{
  "name": "bulog",
  "description": "Stream application logs into a central web-based UI.",
  "homepage": "https://github.com/olavim/bulog#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olavim/bulog.git"
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "bulog": "bin/run.js"
  },
  "version": "0.3.0",
  "scripts": {
    "start": "node ./bin/run.js -p 3000",
    "dev": "tsx watch --ignore vite.config.ts.* ./bin/dev.js -p 3000",
    "clean": "shx rm -rf dist",
    "build": "npm run build:app && npm run build:cli",
    "build:app": "tsc -p tsconfig.app.json && shx rm -rf dist/app && vite build",
    "build:cli": "shx rm -rf dist/cli && tsc -p tsconfig.cli.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clitest": "node ./dummy-app.js | node ./bin/run.js my-app -p 3000"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@iarna/toml": "^2.2.5",
    "@oclif/core": "^3.21.0",
    "@uiw/react-codemirror": "^4.21.24",
    "@uiw/react-json-view": "^2.0.0-alpha.17",
    "beautiful-react-hooks": "^5.0.1",
    "denque": "^2.1.0",
    "env-paths": "^3.0.0",
    "express": "^4.18.3",
    "express-ws": "^5.0.2",
    "json5": "^2.2.3",
    "liqe": "^3.8.0",
    "lodash": "^4.17.21",
    "overlayscrollbars": "^2.5.0",
    "overlayscrollbars-react": "^0.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-resize-detector": "^10.0.1",
    "react-tooltip": "^5.26.3",
    "react-use-websocket": "^3.0.0",
    "react-virtuoso": "^4.7.1",
    "react-window": "^1.8.10",
    "uuid": "^9.0.1",
    "vite-express": "^0.15.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20",
    "@types/node-ipc": "^9.2.3",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "atomic-sleep": "^1.0.0",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "oclif": "^4",
    "postcss": "^8.4.35",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  },
  "files": [
    "bin",
    "dist",
    "node_modules"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "oclif": {
    "bin": "bulog",
    "dirname": "bulog",
    "default": ".",
    "commands": "./dist/cli/commands",
    "topicSeparator": " "
  }
}
