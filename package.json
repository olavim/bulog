{
  "name": "bulog",
  "private": true,
  "type": "module",
  "bin": {
    "bulog": "./bin/run.js"
  },
  "version": "0.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/server/index.js",
    "dev": "tsx watch --clear-screen=false --ignore vite.config.ts.* ./src/server",
    "clean": "shx rm -rf dist",
    "build": "npm run clean && npm run build:app && npm run build:server && npm run build:cli",
    "build:app": "tsc --project tsconfig.app.json && vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:cli": "tsc --project tsconfig.cli.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clitest": "node ./dummy-app.js | node ./bin/run.js my-app -p 3000"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@oclif/core": "^3",
    "@uiw/react-codemirror": "^4.21.24",
    "@uiw/react-json-view": "^2.0.0-alpha.17",
    "beautiful-react-hooks": "^5.0.1",
    "denque": "^2.1.0",
    "express": "^4.18.3",
    "express-ws": "^5.0.2",
    "json5": "^2.2.3",
    "liqe": "^3.8.0",
    "lodash": "^4.17.21",
    "overlayscrollbars": "^2.5.0",
    "overlayscrollbars-react": "^0.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-resize-detector": "^10.0.1",
    "react-tooltip": "^5.26.3",
    "react-use-websocket": "^3.0.0",
    "react-virtuoso": "^4.7.1",
    "react-window": "^1.8.10",
    "uuid": "^9.0.1",
    "vite-express": "^0.15.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20",
    "@types/node-ipc": "^9.2.3",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "atomic-sleep": "^1.0.0",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "oclif": "^4",
    "postcss": "^8.4.35",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "oclif": {
    "bin": "bulog",
    "dirname": "bulog",
    "default": ".",
    "commands": "./dist/cli/commands",
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  }
}
