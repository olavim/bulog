{
	"name": "bulog",
	"description": "Stream application logs into a central web-based UI.",
	"homepage": "https://github.com/olavim/bulog#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/olavim/bulog.git"
	},
	"license": "MIT",
	"type": "module",
	"bin": {
		"bulog": "bin/run.js"
	},
	"version": "0.6.1",
	"scripts": {
		"start": "node ./bin/run.js start",
		"start:test": "node ./bin/run.js start -p 3400 --temp-config",
		"dev": "node ./bin/dev-watch.js start",
		"dev:cli": "tsx ./bin/dev.js",
		"dev:test": "cross-env DISABLE_VITE_HMR=true npm run dev -- -p 3400 --temp-config",
		"clean": "shx rm -rf dist",
		"build": "npm run check:ts && npm run build:app && npm run build:cli",
		"build:app": "tsc -p tsconfig.app.json && shx rm -rf dist/app && vite build && shx cp -r src/app/assets/* dist/app/assets/",
		"build:cli": "shx rm -rf dist/cli/* && node ./bin/build.js",
		"check:ts": "npm run check:ts:app && npm run check:ts:cli",
		"check:ts:app": "tsc -p tsconfig.app.json --noEmit",
		"check:ts:cli": "tsc -p tsconfig.cli.json --noEmit",
		"lint": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
		"format": "prettier . --write --log-level warn",
		"cypress:run": "cypress run -b chrome",
		"cypress:open": "cypress open --e2e -b chrome",
		"test": "start-server-and-test dev:test http://localhost:3400 cypress:run",
		"test:open": "start-server-and-test dev:test http://localhost:3400 cypress:open",
		"test:prod": "start-server-and-test start:test http://localhost:3400 cypress:run",
		"test:prod:open": "start-server-and-test start:test http://localhost:3400 cypress:open",
		"mock:client:stdout": "node ./proto/mock-data.js 100000 | tsx ./bin/dev.js fw bucket-1",
		"prepare": "husky",
		"prepublishOnly": "npm run build && npm run test:prod",
		"audit": "check-audit"
	},
	"dependencies": {
		"@codemirror/lang-javascript": "^6.2.2",
		"@dnd-kit/core": "^6.1.0",
		"@dnd-kit/modifiers": "^7.0.0",
		"@dnd-kit/sortable": "^8.0.0",
		"@iarna/toml": "^2.2.5",
		"@oclif/core": "^3.26.4",
		"@tanstack/react-query": "^5.29.2",
		"@uiw/react-codemirror": "^4.21.25",
		"@uiw/react-json-view": "^2.0.0-alpha.24",
		"axios": "^1.6.8",
		"beautiful-react-hooks": "^5.0.1",
		"cookie-parser": "^1.4.6",
		"cookie-session": "^2.1.0",
		"denque": "^2.1.0",
		"env-paths": "^3.0.0",
		"express": "^4.19.2",
		"express-fileupload": "^1.5.0",
		"immer": "^10.0.4",
		"jose": "^5.2.4",
		"json5": "^2.2.3",
		"liqe": "^3.8.0",
		"lodash": "^4.17.21",
		"mustache-express": "^1.3.2",
		"nanoid": "^5.0.7",
		"openid-client": "^5.6.5",
		"overlayscrollbars": "^2.7.2",
		"overlayscrollbars-react": "^0.5.6",
		"passport": "^0.7.0",
		"proper-lockfile": "^4.1.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.1.0",
		"react-resize-detector": "^10.0.1",
		"react-syntax-highlighter": "^15.5.0",
		"react-tooltip": "^5.26.3",
		"react-use-websocket": "^4.8.1",
		"react-virtuoso": "^4.7.9",
		"react-window": "^1.8.10",
		"signal-exit": "^4.1.0",
		"valid-filename": "^4.0.0",
		"vite-express": "^0.16.0",
		"ws": "^8.16.0",
		"z32": "^1.1.0",
		"zod": "^3.22.5",
		"zod-validation-error": "^3.1.0",
		"zustand": "^4.5.2",
		"zustand-x": "^3.0.3"
	},
	"devDependencies": {
		"@stylistic/eslint-plugin": "^1.7.2",
		"@types/cookie-parser": "^1.4.7",
		"@types/cookie-session": "^2.0.49",
		"@types/express": "^4.17.21",
		"@types/express-fileupload": "^1.5.0",
		"@types/express-session": "^1.18.0",
		"@types/lodash": "^4.17.0",
		"@types/mustache-express": "^1.2.5",
		"@types/node": "^20",
		"@types/passport": "^1.0.16",
		"@types/passport-jwt": "^4.0.1",
		"@types/proper-lockfile": "^4.1.4",
		"@types/react": "^18.2.79",
		"@types/react-dom": "^18.2.25",
		"@types/react-syntax-highlighter": "^15.5.11",
		"@types/react-window": "^1.8.8",
		"@types/ws": "^8.5.10",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"@vitejs/plugin-react": "^4.2.1",
		"@welldone-software/why-did-you-render": "^8.0.1",
		"atomic-sleep": "^1.0.0",
		"autoprefixer": "^10.4.19",
		"chokidar": "^3.6.0",
		"cross-env": "^7.0.3",
		"cypress": "^13.8.0",
		"esbuild": "^0.20.2",
		"esbuild-node-externals": "^1.13.0",
		"eslint": "^8.56.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"husky": "^9.0.11",
		"node-dev": "^8.0.0",
		"oclif": "^3.17.2",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"shx": "^0.3.4",
		"start-server-and-test": "^2.0.3",
		"tailwindcss": "^3.4.3",
		"tsc-alias": "^1.8.8",
		"tsx": "^4.7.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.10"
	},
	"files": [
		"bin",
		"dist",
		"node_modules"
	],
	"engines": {
		"node": ">=18.0.0"
	},
	"oclif": {
		"bin": "bulog",
		"dirname": "bulog",
		"commands": {
			"strategy": "explicit",
			"target": "./dist/cli/index.js",
			"identifier": "commands"
		},
		"helpClass": "./dist/cli/help.js",
		"topicSeparator": " "
	}
}
